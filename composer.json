{
    "name": "steevanb/php-typed-array",
    "license": "MIT",
    "description": "Add IntArray, StringArray, ObjectArray etc.",
    "autoload": {
        "psr-4": {
            "Steevanb\\PhpTypedArray\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Steevanb\\PhpTypedArray\\Tests\\": "tests",
            "Steevanb\\PhpTypedArray\\Bridge\\": "bridge"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "steevanb/php-backtrace": "2.1.*",
        "symfony/config": "6.1.*",
        "symfony/dependency-injection": "6.1.*",
        "symfony/http-kernel": "6.1.*",
        "symfony/property-access": "6.1.*",
        "symfony/serializer": "6.1.*",
        "symfony/string": "6.1.*",
        "symfony/var-dumper": "6.1.*"
    },
    "suggest": {
        "symfony/config": "Needed to use Symfony bridge PhpTypedArrayBundle",
        "symfony/dependency-injection": "Needed to use Symfony bridge PhpTypedArrayBundle",
        "symfony/http-kernel": "Needed to use Symfony bridge PhpTypedArrayBundle",
        "symfony/serializer": "Needede to denormalize TypedArray with PhpTypedArrayBundle",
        "symfony/string": "Needed to use UnicodeStringArray, ByteStringArray or CodePointStringArray."
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-install-cmd": [
            "@rm-composer-lock"
        ],
        "post-update-cmd": [
            "@rm-composer-lock"
        ],
        "rm-composer-lock": "rm composer.lock"
    }
}
